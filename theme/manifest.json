{
  "manifest_version": 3,
  "name": "Theme changer",
  "description": "Change your browser theme.",
  "version": "1",
  "author": "Pengpa0012",
  "action": {
    "default_popup": "index.html",
    "default_icon": "./theme.png"
  },
  "icons": {
    "16": "./theme.png",
    "32": "./theme.png",
    "48": "./theme.png",
    "128": "./theme.png"
  },
  "content_scripts": [
    {
      "matches": ["*://*/*"],
      "js": ["script.js"],
      "css": ["style.css"]
    }
  ]
}
