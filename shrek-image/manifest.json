{
  "manifest_version": 3,
  "name": "Shrek Images",
  "description": "Swap all images with Shrek",
  "version": "1",
  "author": "Pengpa0012",
  "action": {
    "default_popup": "hello.html",
    "default_icon": "/images/shrek-img.png"
  },
  "icons": {
    "16": "/images/shrek-img.png",
    "32": "/images/shrek-img.png",
    "48": "/images/shrek-img.png",
    "128": "/images/shrek-img.png"
  },
  "content_scripts": [
    {
      "matches": ["*://*/*"],
      "js": ["content-script.js"],
      "css": ["style.css"]
    }
  ]
}
